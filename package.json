{
  "name": "muon-visualizer",
  "version": "1.0.0",
  "description": "Offline Muon Visualization Tool",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . muon-visualizer --platform=win32 --arch=x64 --overwrite --package-manager npm",
    "package-linux": "electron-packager . muon-visualizer --platform=linux --arch=x64 --overwrite --package-manager npm"
  },
  "devDependencies": {
    "@electron/packager": "^18.3.5",
    "electron": "^29.1.0"
  }
}
